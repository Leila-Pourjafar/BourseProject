
@{
    ViewBag.Title = "ListData";
}

<div id="app">
    {{ info }}
</div>

<h2>ListData</h2>
@section  scripts{
    <script src="~/Scripts/vue.js"></script>
    <script src="~/Scripts/axios.min.js"></script>
    <script>
        new Vue({
            el: '#app',
            data() {
                return {
                    info: null
                }
            },
            mounted() {
                axios
                    .get('/Employees/ListData')
                    .then(response => (this.info = response))
            }
        })
    </script>
}
